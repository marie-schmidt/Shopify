/*================ Slick dots and prev/next pagination ================*/
.slick-slider .slick-dots {
  
  li {
		margin: 0;
		vertical-align: middle;
		width: $slick-dot-size-small;
		height: $slick-dot-size-small;
		@include rtl-margin-left(8px);
	&:first-of-type {
	  margin-left: 0;
	}
	button {
	  position: relative;
	  padding: 0;
	  width: $slick-dot-size-small;
	  height: $slick-dot-size-small;
	  &:before{
		text-indent: -9999px;
		background-color: transparent;
		border-radius: $slick-dot-size-small;
		background-color: $color_dot;
		width: $slick-dot-size-small;
		height: $slick-dot-size-small;
		transition: all 0.2s;
	  }
	}
	&.slick-active {
		width:20px;
		button{
			width:100%;
			&:before {
				width:100%;
				opacity: 1;
			background: $color-slideshow-dots-active;
			}
		}
	}
	button:active::before {
	  opacity: 0.7;
	}
  }
}
