      background: $nocolor;
      border: $main-border;
      color: #ccc;
      font-size:$theme-font-size;
      @include rtl-float-left;
      @include rtl-margin-right(10px);
      position:relative;
      &:hover, &.change-view--active{
         background: $nocolor;
         color: $theme-text-color;
      }
         .fallback-text{
         display: none;
      }
	}
	.view-grid{
		@include transition(all 0.3s ease);
		border-radius:0;
		&:before{
			content: "";
		   position: absolute;
		   top: 14px;
		   left: 12px;
		   width: 5px;
		   height: 12px;
		   background: #ccc;
		   @include transition(all 0.3s ease);
		}
		&.active,&:hover{
			border-color:$theme-color-default;
			&:before{
				background-color:$theme-color-default;
			}
		}
		&.grid-two{
			&:before{
				@include box-shadow(10px 0 0 #ccc, 10px 0 0 #ccc);
			}
			&.active,&:hover{
            &:before{
            	@include box-shadow(10px 0 0 $theme-color-default, 10px 0 0 $theme-color-default);
            }
			}
		}
		&.grid-three{
