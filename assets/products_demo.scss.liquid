@keyframes goRight {
	from { left: 0; right: 100%; } 
	40% { left: 0; right: 0; }
	60% { left: 0; right: 0; }
	to { left: 100%; right: 0; }
}
.revealedBox-in.goRight {
	> span { 
		height: 100%; 
		top: 0;
	}
}
.revealedBox {
	position:relative;
	.contentBox {
		opacity: 0 !important;
		position: absolute;
		bottom:0;
		top:0;
		left:0;
		right:0;
	} 
	> span {
		position: absolute;
		display: block;   
		background: #f1f1f1; 
	}
}
 
.revealedBox-in {
	> .contentBox {
		opacity: 1 !important;
		transition-delay: 1.05s !important;
	} 
} 

.revealedBox-in.goRight { > span { animation: goRight 1.4s; }}
.revealedBox-in > span { animation-delay: 0.2s; }

