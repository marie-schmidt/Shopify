.dot__show{
  cursor: pointer;
  position:absolute;
  z-index:2;
  width:23px;
  height:23px;
  border-radius:50%;
  //background-color:$theme-color-tertiary;
  @include box-shadow( 0px 2px 4px rgba(0,0,0,0.1));
  @include transition(all 0.3s ease);
  &:before{
	 position:absolute;
	 top:0;
	 left:0;
	 bottom:0;
	 right:0;
	 content:"";
	 @include rgba($white,0.5);
	 z-index: -1;
	 border-radius: 50%;
	 //@include transform(scale(0.8));
	 @include transition(all 0.3s ease);
  }
  &:after{
	content:"";
	position:absolute;
	@include transform(scale(1.8));
	@include square(100%);
	top:0;
	left:0;
  }
  .dot__show_num{
	 font-size:12px;
	 position:absolute;
	 top:0;
	 left:0;
	 bottom:0;
	 right:0;
	 //background-color:$theme-color-tertiary;
	 @include rgba($theme-color-tertiary,0.5);
	 border-radius: 50%;
	 text-align: center;
	 line-height: 24px;
	 color: #fff;
  }
  &:hover,&:active,&:focus{
	 @include box-shadow( 0px 0px 0px 3px rgba(255,255,255,0.5));
	 z-index:10;
	 &:before{
		@include transform(scale(1.7));
	 }
	 .dot__show_num{
		background-color:$theme-color-tertiary;
	 }
  }
  &.light{
		@include box-shadow( 0px 0px 0px 3px rgba(255,255,255,0.5));
	 &:before{
		@include transform(scale(1.7));
	 }
	 .dot__show_num{
		background-color:$theme-color-tertiary;
	 }
  }
  .dot__show-ele{
	 @include opacity(0);
	 @include transform(translate3d(0,-10px,0));
	 @include transition(opacity 0.3s, transform 0.3s);
	 cursor: default;
	 pointer-events: none;
	 @include at-query($max,$screen-tablet - 1 ){
	 	display: none;
	 }
  }
  &:hover,&:active,&:focus{
	 .dot__show-ele{
		@include opacity(1);
		@include transform(translate3d(0,0,0));
		 pointer-events: auto;
	 }
  }
  
}
$arrow-size: 7px;
$arrow-color: #f4f4f4;
.dot__show-ele{
	position:absolute;
	z-index:1;
	&:before{
		content: '';
		 border: solid transparent;
		 height: 0;
		 width: 0;
		 position: absolute;
		 pointer-events: none;
		 border-color: transparent;
		 border-top-color: $arrow-color;
		 border-width: $arrow-size;
		 margin-left: - $arrow-size;
	}
	&.pos--right{
		top:-105px;
		left:150%;
		&:before{
			top: 50%;
			margin-top:- $arrow-size;
			left: 0px;
			border-width: $arrow-size $arrow-size $arrow-size 0;
			border-color: transparent $arrow-color transparent transparent;
		}
	}
	&.pos--left{
		top:-105px;
		right:150%;
		&:before{
			top: 50%;
			margin-top:- $arrow-size;
			right: - $arrow-size;
			border-width: $arrow-size 0 $arrow-size $arrow-size;
				border-color: transparent transparent transparent $arrow-color;
		}
	}
	&.pos--bottom{
		top:150%;
		left:-54px;
		&:before{
			top: - $arrow-size;
			margin-left:- $arrow-size;
			left: 50%;
			border-width: 0 $arrow-size $arrow-size $arrow-size;
			 border-color: transparent transparent $arrow-color transparent;
		}
	}
	&.pos--top{
		bottom:150%;
		left:-54px;
		&:before{
			bottom: - $arrow-size;
			margin-left:- $arrow-size;
			left: 50%;
			border-width: $arrow-size $arrow-size 0 $arrow-size;
				border-color: $arrow-color transparent transparent  transparent;
		}
	}
