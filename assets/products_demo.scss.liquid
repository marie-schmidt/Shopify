	padding-right: 0 !important;
}
.no-margin{
	margin-left:0 !important;
	margin-right:0 !important;
}
.hide {
  display: none !important;
}
.show {
  display: block !important;
}
.invisible {
  visibility: hidden;
}
.text-hide {
