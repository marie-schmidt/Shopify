/* Wishlist Grid
=====================================*/
.wishlist-grid {
  display: none;
  opacity: 0;
  z-index: -1;
  transition: all 0.5s cubic-bezier(0.55, 0.055, 0.675, 0.19);
}

.wishlist-grid.is_visible {
  opacity: 1;
  z-index: 1;
  display: block;
}

.wishlist-grid--empty-list {
  display: none;
  padding: 40px 0px;
  width: 100%;
  max-width: 100%;
  flex-basis: 100%;
  align-items: center;
  justify-content: center;
  opacity: 0;
  z-index: -1;
  height: 0;
  overflow: hidden;
  transition: all 0.5s cubic-bezier(0.55, 0.055, 0.675, 0.19);
}

.wishlist-grid--empty-list.is_visible {
  opacity: 1;
  z-index: 1;
  height: auto;
  display: flex;
}

.empty-list--info {
  text-align: center;
}

.empty-list--text {
  width: 100%;
  max-width: 100%;
  flex-basis: 100%;
  padding-bottom: 20px;
  font-size:em($theme-font-size + 1);
  color:$theme-text-color-secondary;
}
