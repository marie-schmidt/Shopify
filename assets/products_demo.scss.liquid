  // Add a class to ensure links outside the navbar nav are colored correctly.

  .navbar-link {
	color: $navbar-default-link-color;
	&:hover {
	  color: $navbar-default-link-hover-color;
	}
  }

}

// Inverse navbar

.navbar-inverse {
