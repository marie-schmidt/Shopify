/*============================================================================
  Ajaxify Colours
==============================================================================*/
$colorBody: {{ settings.color_body_bg }};
$colorTextBody: {{ settings.color_body_text }};
$colorBorder: {{ settings.color_borders }};

/*============================================================================
  General Styles
==============================================================================*/
.ajaxcart__flip-loader {
	display: none;
	opacity: 0;
	position: absolute;
	right: -24px;
	top: 50%;
	margin-top: -5px;
	padding: 0;
	width: 16px;
	height: 16px;
	@include transition(opacity 0.1s);
	background-repeat: no-repeat;
	background-image: url(data:image/gif;base64,);
	.flip--is-loading & {
		display: block;
		opacity: 1;
	}
}



/*============================================================================
  Ajaxify Cart - General
==============================================================================*/
.ajaxcart__content {
	position: relative;
	background-color: $colorBody;
	@include perspective(1000px);
	@include transform-style(preserve-3d);
	@include transition(all 450ms cubic-bezier(0.57,.06,.05,.95));
	padding-top: $theme-gutter-width;
	padding-bottom: $theme-gutter-width;
	overflow: hidden;
	.is-visible & {
		@include transition(all 550ms cubic-bezier(0.57, 0.06, 0.05, 0.95));
	}

	form {
		margin-bottom: 0;
	}

	@include at-query ($max, $screen-desktop) {
		padding-top: 15px;
		padding-bottom: 15px;
	}

	.cart__header-labels {
		display: none;
	}
}

// Row and column spacing
.ajaxcart__inner {
	//height:100%;
	overflow-y:scroll;
	overflow-x:hidden;
	height:calc(100% - 200px);
	&::-webkit-scrollbar { 
		display: none; 
	}
  .grid--full{
	position: relative;
  }
}

.ajaxcart__product {
	position: relative;
	&:first-child {
		padding-top: 0;
	}
}

.ajaxcart__product,
.cart__row {
	max-height: 500px;
	visibility: visible;
	margin-top: 20px;
	&.is-removed {
		overflow: hidden;
		opacity: 0;
		padding: 0;
		margin: -1px 0 0 0;
		visibility: hidden;
		@include transform-origin(top);
		@include transform('rotateX(-92deg)');
		@include backface-visibility(hidden);
		@include transition(all 450ms cubic-bezier(0.57,.06,.05,.95));
		max-height: 0;
	}
	th{
		text-transform:uppercase;
	}
}

.ajaxcart__row {
	clear: both;
	@include clearfix;
	margin-bottom:30px;
	p{
		margin-bottom: 0;
	}
}

.ajaxcart__product-image {
	display: block;
	overflow: hidden;
	margin-bottom: 15px;
	img {
		display: block;
		max-width: 100%;
	}
}

#CartContainer{
  position: relative;
  height:calc(100% - 60px);
