.map-section__overlay {
  position: relative;
  display: inline-block;
  background-color: $color-bg;
  padding: $section-spacing-small;
  margin-top: $gutter-site-mobile;
  width: 100%;
  text-align: center;
  z-index: 3;

  @include at-query($min,$screen-desktop) {
	position: absolute;
	left: 0;
	top: 50%;
	transform: translateY(30%);
	margin-top: 0;
	width: percentage(1 / 3);

	.ie9 & {
	  top: 10%;
	}
  }

  .map-section--load-error & {
	position: static;
	transform: translateY(0);
  }
}

.map-section__link {
  display: block;
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 2;
}

// Optically center map in visible area with
// extended height/widths and negative margins
.map-section__container {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 150%;
  margin-bottom: -50%;

  @include at-query($min,$screen-desktop) {
	width: 130%;
	height: 100%;
	margin: 0 -30% 0 0;
  }
}
.image-bar__item {
  display: block;
  color: $color-overlay-title-text;
  background: {
	repeat: no-repeat;
	position: 50% 50%;
	size: cover;
  }
}
.image-bar{
  .grid__item{
	overflow: hidden;
	z-index:2;
	.image-bar__item{
		@include transition(all 0.5s ease 0s);
	}
  }
}
.image-bar__content {
  position: relative;
  width: 100%;

  .image-bar--x-small & {
	height: 94px;
  }

  .image-bar--small & {
	height: 280px;
  }

  .image-bar--medium & {
	height: 350px;
  }

  .image-bar--large & {
	height: 480px;
  }

  .image-bar--x-large & {
	height: 580px;
  }

  @include at-query($min,$screen-desktop) {
	.image-bar--x-small & {
	  height: 125px;
	}

	.image-bar--small & {
	  height: 300px;
	}

	.image-bar--medium & {
	  height: 400px;
	}

	.image-bar--large & {
	  height: 630px;
	}

	.image-bar--x-large & {
	  height: 830px;
	}
  }
}

.image-bar__caption {
  position: absolute;
  top: 50%;
  @include transform(translateY(-50%));
  transition: $transition-link-hover;
  width: 100%;
  text-align: center;
}
.collection_banner{
	.image-bar__item{
		overflow:hidden;
		margin-bottom:30px;
		display:block;
		@include transition(all 0.3s ease);
		h3{
			font-size:$theme-font-size + 10;
			font-weight:300;
			margin:0 0 10px 0;
			transition: $transition-link-hover;
			position:relative;
			overflow:hidden;
			&:before{
				position: absolute;
				top: 0;
				left: 0;
				width: 100%;
				height: 100%;
				background: $theme-color-tertiary;
				@include transform(translate3d(-100%,0,0) translate3d(-1px,0,0));
				@include transition(transform 0.7s cubic-bezier(0.7,0,0.3,1));
				content:"";
			}
		}
		.image-bar_bg{
			position:absolute;
			z-index:10;
			top:0;
			left:0;
			right:0;
			bottom:0;
			background-repeat: no-repeat;
			background-position: 50% 50%;
			background-size: cover;
			@include transition(all 0.6s ease);
		}
		.image-bar__sub{
			font-size:12px;
			font-weight:500;
			letter-spacing:1.4px;
			text-transform:uppercase;
			@include transition(all 0.4s ease);
		}
		&:hover{
			h3{
				&:before{
					transform: translate3d(100%,0,0) translate3d(1px,0,0);
				}
			}
			.image-bar__sub{
				color:$theme-color-tertiary !important;
			}
			.image-bar_bg{
				@include transform(scale(1.12));
			}
		}
	}

	.item__cotent{
	  position: absolute;
	  top: 2px;
	  left:2px;
	  text-align: center;
	  padding:25px;
	  @include at-query($min,$screen-lg-desktop){
	  	padding:50px;
	  }
	  z-index:11;
	}

}


