/*================ VARIABLES ================*/
/*============================================================================
  Grid Breakpoints and Class Names
	- Do not change the variable names
==============================================================================*/

$app-brand-prefix        : leo;
$block-prefix                : block;
$block-selector          : block;
$block-heading-selector  : title_block;
$block-content-selector  : block_content;

/***********************************************************************/
// some basic colors
$white:                             #FFFFFF; 
$black:                             #000000;
$blue:                              #6087D2;  // Skin blue
$wild-sand:                         #ffffff;  
$silver:                            #f5f5f5;  
$red:                               #ff0000;
$red-light:                         #ff9999;
$yellow-light:                      #ffdc73;
$nocolor:                           transparent;

// Variables paths 
// ========================================================================

{% if settings.type_base_family == 'google-font' or settings.type_base_family == '' %}
	$font-family:                        {{ settings.type_base }};   //font base body
	//$font-family-tertiary  :             {{ settings.type_header  }};     // font for menu
	{%- if settings.type_header == settings.type_base -%}
	   $font-family-tertiary  :             {{ settings.type_base }};     // font for menu
	{% elsif settings.type_header == '' %}
      $font-family-tertiary  :             {{ settings.type_base }};     // font for menu
   {% else settings.type_header != ''%}
      $font-family-tertiary  :             {{ settings.type_header }};     // font for menu
	{% endif %}
{% else %}
	$font-family:                        {{ settings.theme_font_family | remove: 'Google+' | replace: '+', ' ' }};   //font base body
	$font-family-tertiary  :             {{ settings.theme_font_family_tertiary | remove: 'Google+' | replace: '+', ' ' }};     // font for menu
{% endif %}

//font for slideshow
$font-family-septenary :             $font-family-tertiary;         // font for slideshow
//font for contain product
$font-family-senary    :             $font-family;
//font icon
$font-icon             :             "FontAwesome";

$theme-font-size:                   {{ settings.theme_font_size }};
$theme-font-size-medium:            18px;
//////////////////// Variables defined for Themes + Skins //////////////// 
