#paneltool {
	width: 300px;
	height: 100%;
	position: fixed;
	top: 0px;
	right: -320px;
	left: auto;
	z-index: 1050;
	box-shadow: 0px 1px 3px #999;
	transition-duration: 0.2s;
	transition-property: left, right, top, bottom, width, margin;
