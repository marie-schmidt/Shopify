			.lookv2__title{
				margin-bottom:20px;
			}
			.lookv2__des{
				margin-bottom:50px;
			}
			.lookv2__link{
				text-transform:uppercase;
				font-weight:600;
			}
		}
		&.even{
			@include at-query($min,$screen-md-min){
	         .lookv2__block--text{
	        		@include rtl-float-right();
		         .media{
						@include rtl-padding-left(70px);
				 	}
		      }
		   }
		}
	}
	&.lookv3{
		.look-image{
			margin-bottom: 100px;
			&:last-child{
				margin-bottom: 20px;
			}
		}
		.lookv3__block{
			.lookv2__block--text{
				.lookv2__title{
					margin-bottom: 5px;
				}
            .txt-img{
            	.txt-img-box{
            		position:relative;
            	}
            	@include at-query($min,$screen-sm-min){
	            	.media{
						   z-index: 8;
						   position:absolute;
						   top:-30px;
						   left: -30px;
	            	}
	            }
            	.media-left{
            		margin-bottom: 40px;
            		@include rtl-float-left();
            		@include rtl-margin-right(30px);
            		@include at-query($min,$screen-sm-min){
            			float:none;
            			@include rtl-margin-right(0);
            		}
            	}
            }
			}
			.lookv3__block--img{
				.lookv3__block--box{
					cursor: auto;
				}
			}
			&.even{
				@include at-query($min,$screen-sm-min){
					.lookv2__block--text{
		        		margin-left: auto;
	  		         order: 2;
	  		         .txt-img{
	  		         	padding:0 30px 0 130px;
	  		         }
	  		         .media{
	  		         	padding-left:0;
	  		         }
			      }
			      .lookv3__block--img{
						.lookv3__block--box{
							padding-right: 50px;
							padding-left: 0;
						}
					}
				}
			}
		}
	}
}

