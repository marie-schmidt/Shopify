/* ***************************************************WATH VARIANT PRODCUT***************************************************** */
.swatch{
	margin:20px 0px 0px; 
	.header {
		@include rtl-margin(0px, 10px, 0px, 0px);
		@include rtl-float-left();
		line-height: 35px;
		min-width: 70px;
		text-transform:uppercase;
		font-size:$theme-font-size - 2;
		font-weight:500;
		width:100%;
	}
	input { 
		display:none!important;
	}
	label {
		@include rtl-float-left();
		margin:0;
		border: 1px solid $border-color;
		background-color: $white;
		color: #777;
		/* Styling text */
		font-size:$theme-font-size - 3;
		text-align:center;
		white-space:nowrap;
		text-transform:uppercase;
		@include transition(all 0.3s ease);
		padding:7px 15px;
		z-index:10;
		border-radius:2px;
		font-weight:500;
	}
	.swatch-element {
		@include rtl-float-left();
		-webkit-transform:translateZ(0);
		-webkit-font-smoothing:antialiased;
		margin:2px 18px 0px 0;
		position:relative;
		&.soldout{
			@include opacity(0.2);
			display:none;
		}
	}
}
.swatch-element label {cursor: pointer;}
//.color.swatch-element label { padding:0; }
.swatch input:checked + label {
	//@include box-shadow(0px 1px 4px 1px rgba(0,0,0,0));
	position: relative;
	border-color:$theme-color-default;
	&:after{
		content:"";
		position:absolute;
		background-color:$nocolor;
		position:absolute;
		top:0;
		left:0;
		width:100%;
		height:100%;
		border:2px solid $white;
	}
} 
.swatch{
	&.item_circle{
		.color{
			label{
				border-radius:50%;
				min-width:24px !important; 
			   height:24px !important;
			   padding:0;
            &:after{
            	border-radius:50% !important;
            }
			}
			input{
				&:checked + label{
					@include transform(scale(1.25));
				}
			}
		}
	}
	&.item_square{
		label {
			min-width:34px !important;
			height:34px !important;
			line-height:32px;
			padding-top:0;
			padding-bottom:0;
		}
		.color{
			label{
				border-radius:2px !important;
			   @include transform(none !important);
			   &:before{
			   	line-height:32px !important;
			   	font-size:10px !important;
			   }
			}
		}
	}
	&.item_img{
		label{
			background-position:center;
			background-size:cover;
			&:before{
				display:none !important;
			}
		}
	}
}
.crossed-out { position:absolute; width:100%; height:100%; left:0; top:0; }
.swatch .swatch-element .crossed-out { display:none; }
.swatch .swatch-element.soldout .crossed-out { display:block; }
.swatch .swatch-element.soldout label {
	@include opacity(0.6);
}
.swatch .tooltip {
	text-align:center;
	background:$theme-color-default;
	color:#fff;
	bottom:100%;
	padding: 3px;
	display:block;
	position:absolute;
	width:80px;
	left:-26px;
	margin-bottom:15px;
	visibility:hidden;
	border-radius:2px;
	@include opacity(0);
	//@include transform(translateY(0px));
	@include transition(all .25s ease-out);
	@include box-shadow(2px 2px 6px rgba(0, 0, 0, 0.28));
	@include box-sizing(border-box);
	z-index:-1;
	text-transform:lowercase;
}
.swatch .tooltip:before {
	bottom:-20px;
	content:" ";
	display:block;
	height:20px;
	left:0;
	position:absolute;
	width:100%;
}
.swatch .tooltip:after {
	border-left:solid transparent 10px;
	border-right:solid transparent 10px;
	border-top:solid $theme-color-default 10px;
	bottom:-6px;
	content:" ";
	height:0;
	left:50%;
	margin-left:-13px;
	position:absolute;
	width:0;
}
.swatch .swatch-element:hover .tooltip {
	visibility:visible;
	@include opacity(1);
	
}
.swatch.error {
	background-color:#E8D2D2!important;
	color:#333!important;
	padding:1em;
	border-radius:5px;
}
.swatch.error p {
	margin:0.7em 0;
}
.swatch.error p:first-child {
	margin-top:0;
}
.swatch.error p:last-child {
	margin-bottom:0;
}
.swatch.error code {
	font-family:monospace;
}

/* ***************************************************END WATH VARIANT PRODCUT***************************************************** */

