.collection-grid {
  margin-bottom: -$gutter-site-mobile;
  overflow: hidden;
}

.collection-grid-item {
  position: relative;
  width: 100%;
  padding-bottom: 100%;
  margin-bottom: 40px;

  @include at-query($min,$screen-desktop) {
	margin-bottom: $theme-gutter-width;    
  }
}
.count_items{
  line-height: 40px;
  margin-bottom:10px;
}
.collection-grid-item__title {
  color: $color-overlay-title-text;
  position: absolute;
  text-align: center;
  width: 100%;
  top: 50%;
  padding: 0 5px;
  z-index: 11;
  @include transform(translateY(-50%));
  transition: $transition-link-hover;
  text-shadow: 0 0 15px $color-text-shadow;
  @include at-query($min,$screen-desktop) {
	padding: 0 15px;
  }
}

.collection-grid-item__link {
  border: 2px solid transparent;
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;

  &:focus {
	border-color: $color-image-overlay;
  }
}
.collection-grid-item__overlay {
  position: relative;
  display: block;
  height: 100%;
  width: 100%;
  background-size: cover;
  background-repeat: no-repeat;
  background-position: center top;

}
.collection-grid-item__title-wrapper::before {
  content: '';
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background-color: $color-image-overlay;
  opacity: $opacity-image-overlay;
}

.custom-content {
  @include display-flexbox;
  @include align-items(stretch);
  @include flex-wrap(wrap);
  width: auto;
  margin-bottom: -$theme-gutter-width;
  margin-left: -$theme-gutter-width;

  @include at-query($max,$screen-sm-max) {
	margin-bottom: -$grid-gutter-mobile;
	margin-left: -$grid-gutter-mobile;
  }
}
.custom__item {
  @include flex(0 0 auto);
  margin-bottom: $theme-gutter-width;
  padding-left: $theme-gutter-width;
  max-width: 100%;

  @include at-query($max,$screen-sm-max) {
	@include flex(0 0 auto);
	padding-left: $grid-gutter-mobile;
	margin-bottom: $grid-gutter-mobile;

	&.small--one-half {
	  @include flex(1 0 50%);
	  max-width: 400px;
	  margin-left: auto;
	  margin-right: auto;
	}
  }
  .collection-grid-item {
	margin-bottom: 0;
  }
}
.custom__item-inner {
  position: relative;
  display: inline-block;
  @include rtl-text-align-left ();
  max-width: 100%;
}

.custom__item-inner--video,
.custom__item-inner--collection,
.custom__item-inner--html {
  display: block;
}



/* ========================================   MENU   ============================================================== */

#apollo-menu{
	font-family: $megamenu-font-family;
	background: $megamenu-bg-outsite;
	color: $megamenu-color;
	padding: 0px;
	margin: auto;
}
.apollo-megamenu{
	ul, ol {
		list-style: outside none none;
		margin: 0px;
		padding: 0px;
		position: static;
	}
}
#apollo-top-menu{    
	position: static;
	display: block;
	text-align: center;
	background-color: $megamenu-bg;
	padding:0;
	@include at-query($max,$screen-lg){
		display:none !important;
	}
}
#apollo-menu{
	.navbar{
		background: $megamenu-bg;
		border: 0;
		margin: 0;
		padding: 0;
		@include rounded-corners(0);
	}
	/* menu level 1 */
	.navbar-nav{
	  float: none;
		> li {
		  display: inline-block;
		  float: none;
		  padding:0px;
		  margin:0px;
		  position: relative;
			&:first-child{
				>a{
					@include rtl-padding-left(0);
				}
			}
			&.full-width {
				position: static;
				> .dropdown-menu  {
					width: 100% !important;
				}
			}
			&.aligned-center {
				> .dropdown-menu  {
					left: 0;
					//@include translate(-50%, 0);
					.rtl &{
						//@include translate(50%, 0);
					}
				}
			}
		  &.aligned-center:nth-child(2){
			@include at-query (null,$screen-lg - 176, $screen-lg - 48){
			  > .dropdown-menu  {
				left: 80%;
				@include translate(-80%, 0);
			  }
			}
		  }
			> a{
				position: relative;
				padding: $megamenu-parent-padding;              
				font-weight: $megamenu-font-weight;
				font-size: $megamenu-parent-font-size;
				text-transform:  $megamenu-parent-text-transform;
				background-color: transparent;
				letter-spacing:0.05em;
				color: $megamenu-link-color;
				@include transition(all 350ms ease-out);
				@include at-query($max,$screen-lg-desktop + 200){
					padding-left:15px;
					padding-right:15px;
				}
				&:hover,&:focus,&:active{                   
					color: $megamenu-link-hover-color;              
					background-color: $megamenu-link-bg-hover-color;

				}
			}
			&.dropdown:hover{
				>a{
					&:after{
						border-bottom-width: 6px;
						@include opacity(1);
						@include transition(border 300ms ease-out 0s);
					}
				}
			}
			.caret{
				width:8px;
				border:none;
				&:before{
					font-size: 12px;
					  content: "\f107";
					font-family: $font-icon;
					position: absolute;
					top: 36px;
					color:$theme-text-color-secondary;
					@include rtl-right(18px);
					@include at-query($max,$screen-lg-desktop + 200){
						@include rtl-right(13px);
					}
				}
			}
		}       
	}
	/* level 2 */
	.dropdown-menu{         
		top: 100%;  
		left: 0px;
		display: none;
		visibility: hidden;
		height: auto;
		font-weight: 300;
		color: $megamenu-sub-text-color;
		border: $megamenu-sub-border;
		min-width: $megamenu-sub-min-width;
		padding: $megamenu-sub-padding;
		background-color: $megamenu-sub-bg-color;                                                   
		@include box-shadow($megamenu-sub-shadow);
		-webkit-animation-duration: .35s;
		 -moz-animation-duration: .35s;
		 -o-animation-duration: .35s;
		 animation-duration: .35s;
		 @include animation-name(fadeIn);  
		 -webkit-animation-fill-mode: both;
		 -moz-animation-fill-mode: both;
		 -o-animation-fill-mode: both;
		 animation-fill-mode: both; 
		@include rtl-text-align-left();
		li{
			line-height: 24px;
			padding: 3px 0;
			a{    
				display: block;
				color:$megamenu-sub-link-color;
				font-weight: normal;
				font-size: $megamenu-sub-font-size;
				line-height: 28px; 
				padding:0 42px;        
				@include transition(all 0.3s ease 0s)  ;   
				&:hover{
					background-color: transparent;
					color: $megamenu-sub-link-hover-color;
					@include transform(translateX(5px));
					.fa{
						color: $megamenu-sub-link-hover-color;
					}
				}
				.caret{
					&:before{
						content: "\f105";
						top: 2px;
						right: 20px;
					}
				}
			}           
		}
		.mega-col{
			margin-bottom:10px;
			.mega-col-inner{
				padding:0 15px; 
				.img_blocklinks{
					margin-bottom:20px;
				}
			}
		}      
		.menu-title{
		  padding: 16px 0 20px;
			font-weight: 500;
		  font-size: $theme-font-size - 1;
		  text-transform: uppercase;
		  color: $megamenu-heading-color;
		  letter-spacing: $megamenu-heading-letter;
			a{
			color: $megamenu-heading-color;
				&:hover{color: $megamenu-sub-link-hover-color;}
		  }
		}
		a > .menu-title{
			color: $megamenu-sub-text-color;
		}
	}
	.dropdown-sub{
		padding: 30px;
		li{
			a{
				padding: 0px;
			}
		}
	}
	.dropdown{      
		&:hover{
			> .dropdown-menu{    
								
				visibility: visible;
				display:block;
			}
		}       
	}
	/* level > 3 */
	.dropdown-submenu{
		position: relative;
		.dropdown-menu{                     
			top: 0;
			right: auto;
			left: 120%;                 
			position: absolute;
			display: block;
			visibility: hidden;
			@include opacity(0);
			@include transition(all 0.4s ease);
		}
		
		&:hover{
			> .dropdown-menu{    
				visibility: visible;        
				left: 100%;
				@include opacity(1);
				@include transition(all 0.4s ease);
			}
		}
	}
	.mega-group {       
		& > .dropdown-toggle {          
			border: 0;  
			display: block;         
			letter-spacing: 3px;                
			text-transform: uppercase;                  
			font-family: $megamenu-heading-title-font-family;   
			color: $megamenu-heading-color;         
			.fa{
				color: $white;
			}
		}   
	} 

	.megamenu .cols1{
		min-width:200px;
	}
	.megamenu .cols2{
		min-width:500px;
	}
	.megamenu .cols3{
		min-width:740px;
	}
