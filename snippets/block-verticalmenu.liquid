                                          {%- endif -%}
                                        {%- endif -%}
                                        {%- endfor -%}
                                          {%- if settings[apvermenudesc] != blank -%}
                                                 <span class="menu-desc">{{ settings[apvermenudesc] }}</span>
                                          {%- endif -%}
                                                               </span>
                                                               {%- endif -%}
