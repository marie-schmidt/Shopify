                                                                  <span class="label_submenu" {{ style_label }}>{{ settings[id_submenu_label] }}</span>
                                                                  {%- endif -%}
                                                                {%- endif -%}
                                                                {%- endfor -%}
                                                                                                                                            </a>
                                                                                                                                     </li>
                                                                                                                              {%- endfor -%}
                                                                                                                       </ul>
                                                                                                                </div>
                                                                                                                </div>
                                                                                                  </div>
                                                                                                  </div>
                                                                                           </div>
                                                                                           </li>
