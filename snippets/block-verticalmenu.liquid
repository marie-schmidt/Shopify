                                                                      <a href="{{ link.url }}" title="{{ link.title }}">
                                                                             {%- if settings.vermenuicon_enable -%}
                                                                                    {%- if settings[apvericon] -%}
                                                                                           <span class="menu-icon has-icon menu-icon-{{ forloop.index }}">
                                                                                                  <span class="menu-title">{{ link.title }}</span>
